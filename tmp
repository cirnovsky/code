		for (auto &&[rend, prob] : rends[i]) {
			long long last = 1;
			//dp[i...rend]+=dp[i-1]*pr*prod(1-pr[i~rend-1])
			if (i > 0) last = dp[i - 1];
			//long long tmp = mulmod(last, prob * cum[rend] % MOD * inverse(cum[i]) % MOD * inverse(1 + MOD - prob) % MOD);
			long long tmp = mulmod(last, prob);
			for (int j = i; j < rend; ++j)
				for (auto &&[ignore, prob2] : rends[j])
					tmp = mulmod(tmp, 1 + MOD - prob2);
			tmp = mulmod(tmp, inverse(1 + MOD - prob));
			for (int j = i; j < rend; ++j) addmod(dp[j], tmp);
